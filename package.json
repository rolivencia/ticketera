{
	"name": "ticketera",
	"version": "0.0.1",
	"engines": {
		"node": ">=20.11.0 <21.0.0"
	},
	"type": "module",
	"scripts": {
		"preinstall": "npx only-allow pnpm",
		"postinstall": "pnpm run run-migrations",
		"dev": "concurrently -p --names=\"Server build,Frontend app,Server app\" --prefix-colors=\"yellow,red,green\" \"pnpm run build:watch\" \"pnpm run serve:frontend\" \"pnpm run serve:backend\"\n",
		"serve:frontend": "nx run ticketera:serve:development",
		"serve:backend": "node -r ts-node/register --watch-path=./dist/ticketera/server --env-file=.env dist/ticketera/server/server.mjs",
		"build": "nx run ticketera:build:production --skip-nx-cache",
		"build:watch": "nx run ticketera:build:production --skip-nx-cache --watch",
		"storybook": "nx run ticketera:storybook",
		"storybook:build": "nx run ticketera:build-storybook",
		"test": "nx test",
		"test:watch": "nx test --watch",
		"sanity:dev": "cd cms && sanity dev",
		"prepare": "husky",
		"create-migrations": "pnpm drizzle-kit generate:pg",
		"run-migrations": "pnpm tsx --env-file=.env run-migrations.ts"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "17.3.3",
		"@angular/common": "17.3.3",
		"@angular/compiler": "17.3.3",
		"@angular/core": "17.3.3",
		"@angular/forms": "17.3.3",
		"@angular/platform-browser": "17.3.3",
		"@angular/platform-browser-dynamic": "17.3.3",
		"@angular/platform-server": "17.3.3",
		"@angular/router": "17.3.3",
		"@angular/ssr": "17.3.3",
		"@nx/angular": "18.2.3",
		"@types/cors": "^2.8.17",
		"@types/express": "^4.17.17",
		"angularx-qrcode": "^17.0.0",
		"cors": "^2.8.5",
		"drizzle-orm": "^0.30.10",
		"esbuild": "0.19.11",
		"express": "^4.18.2",
		"postgres": "^3.4.4",
		"rxjs": "~7.5.0",
		"tslib": "^2.3.0",
		"zone.js": "0.14.3"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "17.3.3",
		"@angular-eslint/eslint-plugin": "17.3.0",
		"@angular-eslint/eslint-plugin-template": "17.3.0",
		"@angular-eslint/template-parser": "17.3.0",
		"@angular/cli": "~17.3.3",
		"@angular/compiler-cli": "17.3.3",
		"@angular/language-service": "17.3.3",
		"@nx/cypress": "18.2.3",
		"@nx/eslint-plugin": "18.2.3",
		"@nx/jest": "18.2.3",
		"@nx/js": "18.2.3",
		"@nx/linter": "18.2.3",
		"@nx/storybook": "18.2.3",
		"@nx/workspace": "18.2.3",
		"@storybook/addon-essentials": "7.5.3",
		"@storybook/addon-styling": "^1.3.0",
		"@storybook/angular": "7.5.3",
		"@storybook/core-server": "7.5.3",
		"@testing-library/angular": "^16.0.0",
		"@types/jest": "29.4.4",
		"@types/node": "^18.16.9",
		"@types/react": "^18.2.11",
		"@typescript-eslint/eslint-plugin": "7.3.0",
		"@typescript-eslint/parser": "7.3.0",
		"autoprefixer": "^10.4.18",
